{
  "name": "lazymesh",
  "sourceRoot": "packages/lazymesh/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "packages/lazymesh"
      },
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test:run",
        "cwd": "packages/lazymesh"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run lint",
        "cwd": "packages/lazymesh"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run format",
        "cwd": "packages/lazymesh"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev",
        "cwd": "packages/lazymesh"
      }
    }
  },
  "tags": [
    "type:lib"
  ]
}