{
  "name": "simon-would-have-said",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit build && svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "deploy": "svelte-kit build && node ./deploy.js",
    "test": "jest",
    "precommit": "npm run lint && npm run check && npm test"
  },
  "devDependencies": {
    "@sveltejs/kit": "next",
    "@types/d3": "^6.7.0",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "jest": "^29.3.1",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "ts-jest": "^29.0.3",
    "tslib": "^2.0.0",
    "typescript": "^4.7.3"
  },
  "type": "module",
  "dependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@sveltejs/adapter-static": "next",
    "d3": "^7.0.0",
    "katex": "^0.13.11",
    "mdsvex": "^0.9.7",
    "nodejs-polars": "^0.6.0",
    "prismjs": "^1.23.0",
    "rehype-katex": "^5.0.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-footnotes": "^2.0.0",
    "remark-math": "^3.0.1",
    "sass": "^1.34.1",
    "svelte-vega": "^1.1.0",
    "vega": "^5.20.2",
    "vega-embed": "^6.18.2",
    "vega-lite": "^5.1.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^d3$": "<rootDir>/node_modules/d3/dist/d3.min.js"
    }
  }
}
